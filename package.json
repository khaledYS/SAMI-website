{
  "name": "ramadan",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "vite:dev" : "vite",
    "vite:build": "vite:build",
    "tailwind:dev": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwindOutput.css --watch",
    "tailwind:build": "npx tailwindcss -o ./src/styles/tailwindOutput.css --minify",
    "dev": "start npm run tailwind:dev && vite",
    "build": "npm run tailwind:build && npm run vite:build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "vite": "^2.8.0"
  }
}
